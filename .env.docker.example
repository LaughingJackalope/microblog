# Docker Compose Environment Variables
# Copy to .env for local Docker development

# Python Backend Secret (generate with: openssl rand -base64 32)
PYTHON_SECRET_KEY=your-python-secret-key-here-min-32-characters

# Next.js Session Secret (generate with: openssl rand -base64 32)
NEXTJS_SESSION_SECRET=your-nextjs-session-secret-here-min-32-characters

# Environment
ENVIRONMENT=development
NODE_ENV=development
